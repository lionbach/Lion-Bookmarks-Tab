{
  "manifest_version": 3,
  "name": "Lion Bookmarks Tab",
  "version": "1.0",
  "description": "Lee y muestra los favoritos del usuario en una nueva pestaña.",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "Abrir página"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "img/bookmark.png",
    "48": "img/bookmark.png",
    "128": "img/bookmark.png"
  }
}
