{
  "manifest_version": 3,
  "name": "Lion Bookmarks Tab",
  "version": "1.0",
  "description": "Browse your favorites and folders from the new tab screen.",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "Lion Bookmarks Tab"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "img/bookmarks-color16.png",
    "32": "img/bookmarks-color32.png",
    "48": "img/bookmarks-color48.png",
    "64": "img/bookmarks-color64.png",
    "128": "img/bookmarks-color128.png"
  }
}
